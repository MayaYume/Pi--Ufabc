import math

def b ( z : int ) -> int :
  prod = a (z , z )
  print (z , prod )
  return prod

def a ( x : int , y : int ) -> int :
  x = x + 1
  return x * y

def c ( x : int , y : int , z : int ) -> int :
  total = x + y + z
  quadr = b ( total ) **2
  return quadr
x = 1
y = x + 1
print ( c (x , y +3 , x + y ) )
